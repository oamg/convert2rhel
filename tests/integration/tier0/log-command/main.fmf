summary: Passed command at the top of a log file
description: |
    Verify log file is generated and the passed command is in first line of the log file.

tier: 0

tag+:
    - log-file
    - log-command

/logfile_starts_with_command:
    tag+:
        - logfile-starts-with-command
    test: |
        pytest -svv -m logfile_starts_with_command
